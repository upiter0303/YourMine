{{>layout/header}}
<link rel="stylesheet" href="/css/account.css"/>
{{>layout/nav}}
<div class="account-form">
    <div id="account-logo">
        회원정보 변경
    </div>
    <form>
        <div class="form-floating input-group mb-3">
            <label for="no">회원번호</label>
            <input type="text" class="form-control" id="no" value="{{user.no}}" readonly>
        </div>
        <div class="form-floating input-group mb-3">
            <label for="id">아이디</label>
            <input type="text" class="form-control" id="id" value="{{user.id}}" readonly>
        </div>
        <div class="form-floating input-group mb-3">
            <label for="name">이름</label>
            <input type="text" class="form-control" id="name" value="{{user.name}}">
        </div>
        <div class="form-floating input-group mb-3">
            <label for="email">이메일</label>
            <input type="text" class="form-control" id="email" value="{{user.email}}">
        </div>
        <div class="form-floating input-group mb-3">
            <label for="score">매너온도</label>
            <input type="text" class="form-control" id="score" value="{{user.score}}">
        </div>
    </form>
    <a href="/adminPage" role="button" class="btn btn-secondary">취소</a>
    <button type="button" class="btn btn-primary" onclick="usersModify();">정보 등록</button>
</div>
{{>layout/footer}}
{{>layout/js}}
<script src="//t1.daumcdn.net/mapjsapi/bundle/postcode/prod/postcode.v2.js"></script>
<script src="/js/admin.js"></script>
</body>
</html>